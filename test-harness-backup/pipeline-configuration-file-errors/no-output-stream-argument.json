{
  "id" : "freebayes",
  "configuration type" : "pipeline",
  "description" : "run freebayes",
  "categories" : ["Variant-discovery"],

  "pipeline tasks" : [
    {
      "task" : "call",
      "tool" : "freebayes-no-stream",
      "output to stream" : true
    },
    {
      "task" : "compress",
      "tool" : "bgzip",
      "omit from reduced plot" : true
    }
  ],

  "import arguments" : "call",

  "arguments" : {
  },

  "unique graph nodes" : [
    {
      "id" : "bam",
      "task" : "call",
      "task argument" : "--in",
      "is greedy" : true
    },
    {
      "id" : "reference",
      "task" : "call",
      "task argument" : "--fasta-reference"
    },
    {
      "id" : "region",
      "task" : "call",
      "task argument" : "--region"
    }
  ],

  "shared graph nodes" : [
    {
      "id" : "out",
      "arguments sharing node" : [
        {
          "task" : "call",
          "task argument" : "--out"
        },
        {
          "task" : "compress",
          "task argument" : "--in"
        }
      ]
    }
  ],

  "parameter sets" : [
    {
      "id" : "default",
      "description" : "Default parameter set requires user to input all input filenames.",
      "data" : [
        {
          "id" : "region",
          "node" : "region",
          "values" : [20]
        }
      ]
    }
  ]
}
